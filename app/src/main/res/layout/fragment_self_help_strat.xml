<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/placeholderResources"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4EFFC"
    tools:context=".fragments.resourcesFragments.SelfHelpStrat">


    <TextView
        android:id="@+id/MoodTitle"
        android:layout_width="342dp"
        android:layout_height="0dp"
        android:layout_marginTop="45dp"
        android:fontFamily="@font/poppins_medium"
        android:includeFontPadding="true"
        android:text="Self Help"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="#081042"
        android:textSize="34sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.492"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TeenTalk"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="520dp"
        android:text="Breathing Exercises"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/StressLessZone"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="480dp"
        android:text="Gratitude Journaling"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView8"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/bipolarBud"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="400dp"
        android:text="Sleep Tracker"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/DepressionFighters"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="320dp"
        android:text="Goal Tracker"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/traumaSurvivors"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="250dp"
        android:text="Self Reflection"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/socialConnections"
        android:layout_width="250dp"
        android:layout_height="40dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="40dp"
        android:layout_marginBottom="180dp"
        android:text="Social Connections"
        android:textSize="21sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/crisisNumber"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="540dp"
        android:text="www.breathing.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/podcastLink"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="450dp"
        android:text="www.journal.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="0.941" />

    <TextView
        android:id="@+id/workbookLink"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="380dp"
        android:text="www.tracker.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/toolkitLinks"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="300dp"
        android:text="www.goaltracker.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/sleephygieneLinks"
        android:layout_width="160dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="230dp"
        android:text="www.selfreflection.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <TextView
        android:id="@+id/onlineCoursesLinks"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:layout_marginBottom="160dp"
        android:text="www.social.com"
        android:textSize="15sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/TeenTalk"
        app:layout_constraintTop_toBottomOf="@+id/MoodTitle"
        app:layout_constraintVertical_bias="1.0" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="84dp"
        android:layout_height="100dp"
        android:layout_marginTop="15dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="70dp"
        android:src="@drawable/help_icon"
        app:layout_constraintBottom_toTopOf="@+id/StressLessZone"
        app:layout_constraintEnd_toEndOf="@+id/MoodTitle"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>